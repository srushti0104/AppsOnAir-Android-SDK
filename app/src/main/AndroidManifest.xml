<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.appupdate.appupdateproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppUpdateProject">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.appsonair.icon"
            android:resource="@drawable/maintenance_icon" />
        <meta-data
            android:name="com.appsonair.name"
            android:value="@string/app_name" />

        <!--  Add your Appsonair app id here => Get your appId from https://appsonair.com/ -->
        <meta-data
            android:name="app_id"
            android:value="********-****-****-****-************" />

    </application>

</manifest>